        try:
            cursor.execute(
                'INSERT INTO bot_accounts (platform, username, password, email, created_date, health_score) VALUES (?, ?, ?, ?, ?, ?)',
                (platform, username, password, email, datetime.now().isoformat(), 100)  # Start with 100 health
            )
            conn.commit()
