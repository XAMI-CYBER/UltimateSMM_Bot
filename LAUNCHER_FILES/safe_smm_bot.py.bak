class PasswordManager:
    def __init__(self):
        self.admin_password_file = "admin_pass.key"
        self.setup_password()
    
    def hash_password(self, password):
        """Password hash ‡¶ï‡¶∞‡¶¨‡ßá"""
        return hashlib.sha256(password.encode()).hexdigest()
    
    def setup_password(self):
        """Admin password setup"""
        if not os.path.exists(self.admin_password_file):
            print("\nüîê FIRST TIME SETUP - ADMIN PASSWORD")
            print("="*50)
            print("Set your admin password for UltimateSMM Bot")
            print("This password will be required to access the system")
            print("="*50)
            
            while True:
                password = input("Enter ADMIN PASSWORD: ").strip()
                confirm = input("Confirm ADMIN PASSWORD: ").strip()
                
                if password == confirm and len(password) >= 4:
                    hashed = self.hash_password(password)
                    with open(self.admin_password_file, 'w') as f:
                        f.write(hashed)
                    print("‚úÖ ADMIN PASSWORD SET SUCCESSFULLY!")
                    print("‚ö†Ô∏è Remember this password - it cannot be recovered!")
                    break
                else:
                    print("‚ùå Passwords don't match or too short! Minimum 4 characters.")
    
    def verify_password(self):
        """Password verify ‡¶ï‡¶∞‡¶¨‡ßá"""
        print("\nüîê ULTIMATE SMM BOT - LOGIN")
        print("="*40)
        
        attempts = 3
        while attempts > 0:
            password = input("Enter ADMIN PASSWORD: ").strip()
            hashed_input = self.hash_password(password)
            
            with open(self.admin_password_file, 'r') as f:
                stored_hash = f.read().strip()
            
            if hashed_input == stored_hash:
                print("‚úÖ ACCESS GRANTED!")
                return True
            else:
                attempts -= 1
                print(f"‚ùå Wrong password! {attempts} attempts remaining.")
        
        print("üö´ ACCESS DENIED! Too many failed attempts.")
        return False

